<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tianbo.smartcity.modules.your.dao.mapper.SensorTypeMapper">

    <select id="getListBySystemType" resultType="com.tianbo.smartcity.modules.your.entity.SensorType">
        select * from t_sensor_type
        <if test="systemType != null and systemType != ''">
            where system_type = #{systemType}
        </if>
    </select>
</mapper>